{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET" }
      ]
    }
  ],
  "functions": {
    "api/matches.js": {
      "includeFiles": "{data,lib}/**"
    },
    "api/match/[cast]/[id].js": {
      "includeFiles": "{data,lib}/**"
    }
  }
}
